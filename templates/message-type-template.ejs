export type Consumers = '<%- consumers.join(
  "' | '"
) %>'

export type Actions = {
  <%- actions.map(action => {
    return `${action.consumer}: '${action.actions.join("' | '")}'`
  }).join(',\n') %>
};

// Estrutura b√°sica de uma mensagem
export type Message<T extends Consumers> = {
  consumer: T;
  action: Actions[T];
  payload: any;
};
